{
	"name": "codex-openai-wrapper",
	"version": "0.0.0",
	"private": true,
	"packageManager": "npm@11.6.0",
	"scripts": {
		"prepare:wrangler-config": "node scripts/prepare-wrangler-config.mjs",
		"deploy": "npm run prepare:wrangler-config && wrangler deploy --config .wrangler.generated.toml",
    "dev": "npm run prepare:wrangler-config && wrangler types && wrangler dev --config .wrangler.generated.toml",
		"start": "npm run dev",
		"build:node": "esbuild src/server-node.ts --bundle --format=esm --platform=node --target=node22 --outfile=dist/server-node.mjs",
		"start:node": "node dist/server-node.mjs",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"cf-typegen": "wrangler types",
		"postinstall": "wrangler types",
		"format": "prettier --write src",
		"format:check": "prettier --check src",
		"build": "npm run prepare:wrangler-config && wrangler deploy --dry-run --outdir=dist --config .wrangler.generated.toml",
		"tsc": "tsc --noEmit --skipLibCheck",
		"lint": "eslint --ext .ts src && npm run tsc",
		"lint:fix": "eslint --ext .ts src --fix",
		"gen:env": "node scripts/generate-env.mjs"
	},
	"devDependencies": {
		"esbuild": "^0.23.0",
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.32.0",
		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.39.1",
		"@typescript-eslint/parser": "^8.39.1",
		"eslint": "^9.34.0",
		"prettier": "^3.6.2",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.37.1"
	},
	"dependencies": {
		"@hono/node-server": "^1.11.0",
		"hono": "^4.9.2"
	}
}
